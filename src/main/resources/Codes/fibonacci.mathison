proc main(){
    num input
    leer(input)
    imprimir(fiboRec(input))
}

num fiboRec ( num n ) {
    num resultado
    si ( n <= 1 ) //CASO BASE
	{
	    resultado = 1
    }
    sino          //CASO TRIVIAL
	{
        num enesimo = fiboRec ( n - 1 ) + fiboRec ( n - 2 )
	    resultado = enesimo
	}
	devolver resultado
}