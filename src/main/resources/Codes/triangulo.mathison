publico proc imprimirTriangulo()
{
	imprimir("Digite el caracter que desea imprimir en un triangulo\n")
	cad c
	leer(c)
	imprimir("Digite el numero de filas que desea que tenga el triangulo\n")
	num n
	leer(n)
	cad fila = ""
	para num i desde 1 hasta n avance 1
    {
        para num j desde 1 hasta i avance 1
        {
            fila = fila + c
            imprimir(fila)
        }
        imprimir("\n")

    }
}